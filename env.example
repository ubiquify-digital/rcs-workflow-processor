# AWS Credentials (Option 2: Environment Variables)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_DEFAULT_REGION=us-east-1

# Optional: Specific S3 endpoint (for custom S3-compatible services)
# AWS_S3_ENDPOINT_URL=https://s3.amazonaws.com

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Roboflow Configuration (update these with your actual values)
ROBOFLOW_API_KEY=your_roboflow_api_key_here
ROBOFLOW_WORKSPACE=your_workspace_name
ROBOFLOW_WORKFLOW_ID=your_workflow_id

# OpenAI Configuration (for license plate OCR workflows)
OPENAI_API_KEY=your_openai_api_key_here

# S3 Output Configuration
DEFAULT_OUTPUT_BUCKET=your-output-bucket-name
ALLOW_CUSTOM_OUTPUT_BUCKET=false

# Video Processing Configuration
ENABLE_FRAME_DROP_ON_VIDEO_FILE_RATE_LIMITING=True
